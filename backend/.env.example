NODE_ENV=development
PORT=4000

# WeatherAPI.com Configuration
# Get your API key from: https://www.weatherapi.com/signup.aspx
WEATHER_API_KEY=<YOUR_WEATHERAPI_KEY>
WEATHER_API_PROVIDER=weatherapi

# Supabase Configuration (Backend - Service Role Key)
# IMPORTANT: Service role key should NEVER be exposed to frontend
SUPABASE_URL=<YOUR_SUPABASE_URL>
SUPABASE_SERVICE_ROLE_KEY=<YOUR_SUPABASE_SERVICE_ROLE_KEY>

# JWT Secret (for additional security if needed)
JWT_SECRET=<random_jwt_secret_for_session_signing_if_needed>
